#include "RP_InterruptManager.h"

#include "system_bluenrg2.h"

void rp_im_init()
{

}

/*
 * Bluetooth ISR
 */
void Blue_Handler(void)
{
	sam_bl_notifyEvent();
}

/*
 * UART ISR
 */
void UART_Handler(void)
{
	hw_uart_receiveISR();
}
