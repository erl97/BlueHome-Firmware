#include "DB_TestCases.h"

#include <stdint.h>

#include "SDK_EVAL_Led.h"

void db_tc_init_Eval()
{
	SdkEvalLedInit(LED1);
	SdkEvalLedInit(LED2);
	SdkEvalLedInit(LED3);

	SdkEvalLedOff(LED1);
	SdkEvalLedOff(LED2);
	SdkEvalLedOff(LED3);
}

void db_tc_test_EvalLeds()
{
	for (int i = 0; i < 10; i++)
	{
		SdkEvalLedToggle(LED3);
		for (uint32_t j = 0; j < 1000000; j++)
		{
		__NOP();
		}
	}
}
